                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <!--#endregion-->

                                <div class="row mt-3">
                                    <div class="col-lg-12 mb-3">
                                        @foreach (var item in Model.VehicleList)
                                        {

                                            <div style="border:1px solid #b7b7b7;" class="mb-3">
                                                <div class="col-12 p-2">

                                                    <div class="row">
                                                        <div class="col-12">

                                                            <span><strong>@item.Name</strong></span>
                                                        </div>
