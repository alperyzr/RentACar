                                                                                        <span>
                                                                                            <a href="#" type="button" data-toggle="modal" data-target="#PointScale">@Html.Raw(Arackiralama.Resources.LanguageFields.PointButtonVeryNice)</a>

                                                                                        </span>

                                                                                        @{ Html.RenderAction("_PointScale", "Car");}


                                                                                    </div>*@
                                                                                @*<div class="col-lg-4">
