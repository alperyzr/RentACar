@model Arackiralama.Models.VehicleSearchModel
@{
    ViewBag.Title = "CarList";

    //List<Arackiralama.Models.VehicleModel> uniqCars=new List<Arackiralama.Models.VehicleModel>();
    //uniqCars = Model.VehicleList.OrderBy(t => t.TotalRate).GroupBy(p => p.CategoryID).Select(g => g.First()).ToList();

    var mini = Model.VehicleList.OrderBy(t => t.TotalRate).Where(p => p.CategoryID == 1).FirstOrDefault();
    var economi = Model.VehicleList.OrderBy(t => t.TotalRate).Where(p => p.CategoryID == 3).FirstOrDefault();
    var standart = Model.VehicleList.OrderBy(t => t.TotalRate).Where(p => p.CategoryID == 7).FirstOrDefault();
    var Premium = Model.VehicleList.OrderBy(t => t.TotalRate).Where(p => p.CategoryID == 10).FirstOrDefault();
}
<body>
    <div class="row">
        <div class="col-md-3 mb-1">
            <button id="active-button1" style="width:100%;" class="btn btn-step btn-save" type="button" data-toggle="collapse" data-target="#edit-step1"><span style="color:#fff;">1.</span> @Html.Raw(Arackiralama.Resources.LanguageFields.SearchResults)</button>
        </div>
        <div class="col-md-3 mb-1">
            <button id="active-button2" style="width:100%;" disabled="disabled" class="btn btn-step btn-save collapsed" type="button" data-toggle="collapse" data-target="#edit-step2"><span style="color:#fff;">2.</span> @Html.Raw(Arackiralama.Resources.LanguageFields.Options)</button>
        </div>
        <div class="col-md-3 mb-1">
            <button id="active-button3" style="width:100%;" disabled="disabled" class="btn btn-step btn-save collapsed" type="button" data-toggle="collapse" data-target="#edit-step3"><span style="color:#fff;">3.</span> @Html.Raw(Arackiralama.Resources.LanguageFields.Details)</button>
        </div>
        <div class="col-md-3 mb-1">
            <button id="active-button4" style="width:100%;" disabled="disabled" class="btn btn-step btn-save collapsed" type="button" data-toggle="collapse" data-target="#edit-step4"><span style="color:#fff;">4.</span>@Html.Raw(Arackiralama.Resources.LanguageFields.Confirmation)</button>
        </div>
    </div>
</body>

