                                                                                <span>
                                                                                    @Html.Raw(Arackiralama.Resources.LanguageFields.TransmissionType)
                                                                                    <strong>@item.TransmissionType</strong>
                                                                                </span>
                                                                            </span>
                                                                        </div>
                                                                        <div class="col-12 mt-2">
                                                                            <span>
                                                                                <i class="fas fa-gas-pump mr-1"></i>

                                                                                <span>
                                                                                    @Html.Raw(Arackiralama.Resources.LanguageFields.FuelType)
                                                                                    <strong>@item.FuelType</strong>
                                                                                </span>
                                                                            </span>
                                                                        </div>

                                                                        <div class="col-12 mt-2">
