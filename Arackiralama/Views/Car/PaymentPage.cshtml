@using Arackiralama.Models;
@model Arackiralama.Models.VehicleSearchModel
@{
    ViewBag.Title = "PaymentPage";

    string[] selectedExtraIDs = Model.extraFeeHdn.Split(',');
    string totalExtras = string.Empty;
    foreach (var item in Model.Vehicle.IncludePrice)
    {
        for (int i = 0; i < selectedExtraIDs.Length; i++)
        {

            if (!string.IsNullOrEmpty(selectedExtraIDs[i]))
            {


                if (selectedExtraIDs[i] == item.Item4)
                {
                    totalExtras = totalExtras + item.Item1 + ' ' + item.Item2 + "<br/>";
                }
            }
        }
    }
    Accounts LoginUser = new Accounts();
    AracKiralamaEntities1 db = new AracKiralamaEntities1();

    if (Session["KullaniciAdi"] != null)
    {
        int UID =Convert.ToInt32(Session["ID"].ToString());
        LoginUser = db.Accounts.FirstOrDefault(u => u.ID == UID);
    }
   

}
