			alert("@(Html.Raw(Arackiralama.Resources.LanguageFields.MailUnavaible))");
			i++;
		}*@

        var checkControl = $("input[name=onBilgilendirme]:checked").length > 0;
        //var campControl = $("input[name=onBilgilendirme]:checked").length > 0;

        if (checkControl == 0) {

