    <script>
        function CultureIDOnChangeHandler() {

            var select = document.getElementById('cultureID');
            var index = select.selectedIndex;
            location.href = '/Car/CultureChange?Culture=' + select.options[index].value;
        }

        function CurrencyIDOnChangeHandler() {

            var select = document.getElementById('currencyID');
            var index = select.selectedIndex;
            location.href = '/Car/CurrencyChange?Currency=' + select.options[index].value;
        }


    </script>


