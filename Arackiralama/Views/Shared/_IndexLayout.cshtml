    @if (Session["culture"].ToString() == "tr-TR")
    {
        ViewBag.Culture = "tr";
    }
    else if (Session["culture"].ToString() == "en-GB")
    {
        ViewBag.Culture = "en";
    }
    else if (Session["culture"].ToString() == "ru-RU")
    {
        ViewBag.Culture = "ru";
    }
    else
    {
        ViewBag.Culture = "tr";
    }
    @if (Session["culture"] != "null")
    {
        if (Session["culture"].ToString() == "en-GB")
        {
            <script src="~/scripts/datepicker/zebra_datepicker_en.js"></script>


        }
        else
        {
            <script src="~/scripts/datepicker/zebra_datepicker.js"></script>

        }
    }
    else
    {
        <script src="~/scripts/datepicker/zebra_datepicker.js"></script>
    }
